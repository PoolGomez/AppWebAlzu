{
  "compilerOptions": {
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts", "app/(routes)/components/GraphicSuscribers", "app/(routes)/alzu/[companyId]/fabric/SnappingHelpers.jsx", "app/(routes)/alzu/[companyId]/fabric/Settings.jsx", "app/(routes)/alzu/[companyId]/fabric/page.jsx", 
  // "app/(routes)/alzu/[companyId]/fabric/page.jsx"
],
  "exclude": ["node_modules"]
}
